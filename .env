# isi dengan nomor yang ingin anda jadikan bot, kode pairing muncul di log
PAIRING_NUMBER=

# nomor owner untuk penggunaan command khusus owner
OWNER=["6285815663170"]

# untuk size auto restart, RAM sisa 350MB auto restart
AUTO_RESTART=350 MB

# Aktifkan atau nonaktifkan fitur auto restart (true atau false)
AUTO_RESTART=true

# `true` untuk pribadi dan `false` untuk publik
SELF=true

# opsional
WRITE_STORE=false

# session name
SESSION_NAME=session

# sticker pack
packName=Sticker Dibuat Oleh : 
packPublish=Dika Ardnt.

# emoji untuk auto react status, pisahkan dengan koma (,) dan kosongkan jika tidak perlu
REACT_STATUS=❤️,💀,😋,😊,😒,🔥

# opsional jikalau perlu, untuk backup status ke telegram
# token bot telegram bisa diambil ke https://t.me/BotFather
# ambil id telegrammu di https://t.me/getmyid_bot
TELEGRAM_TOKEN=
ID_TELEGRAM=

# kecepatan auto read story dalam milidetik
AUTO_READ_SPEED=1000

# aktifkan atau nonaktifkan fitur auto typing (true atau false)
AUTO_TYPING=true
